    <h2>Your contact page.</h2>
</hgroup>

<section class="contact">
    <header>
        <h3>Phone:</h3>
    </header>
    <p>
        <span class="label">Main:</span>
        <span>823.555.0100</span>
    </p>
    <p>
        <span class="label">After Hours:</span>
        <span>823.555.0199</span>
    </p>
