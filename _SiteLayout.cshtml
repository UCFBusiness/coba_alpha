        <script>
            $(function () { //load on document.ready

                //----- Easing Navigation ------------------------------
                //-- 1 -- build header when page is first loaded
                content_buildHeaders();
                //-- 2 -- resize header to match window size
                $(window).resize(content_resizeHeaders);
                //-- 3 -- update header position when window is scrolled
                $(window).scroll(content_updateHeaders).trigger("scroll");
                //-- 4 in requireJS
                //------------------------------------------------------

            });
        </script>

        <script>
            //----- Easing Functions ------------------------------------

            function content_buildHeaders() {
                $(".persist-area").each(function () {
                    var clonedHeaderRow;
                    clonedHeaderRow = $(".persist-header", this);
                    clonedHeaderRow
                        .before(clonedHeaderRow.clone())
                        .css("width", clonedHeaderRow.width())
                        .addClass("floatingHeader");
                });
            }

            function content_resizeHeaders() {
                $(".persist-area").each(function () {
                    var clonedHeaderRow;
                    clonedHeaderRow = $(".persist-header", this);
                    clonedHeaderRow.css("width", "100%"),
                    clonedHeaderRow.css("width", clonedHeaderRow.width())
                });
            }

            function content_updateHeaders() {
                $(".persist-area").each(function () {
                    var el = $(this),
                    offset = el.offset(),
                    scrollTop = $(window).scrollTop(),
                    floatingHeader = $(".floatingHeader", this)

                    if ((scrollTop > offset.top) && (scrollTop < offset.top + el.height())) {
                        floatingHeader.css({"visibility": "visible"});
                    } else {
                        floatingHeader.css({"visibility": "hidden"});
                    };
                });
            }
        </script>


